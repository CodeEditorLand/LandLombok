{
	"name": "vscode-lombok",
	"displayName": "Lombok Annotations Support for VS Code",
	"version": "0.0.1",
	"private": false,
	"description": "Refactor code with Lombok annotations, or remove Lombok annotations with actual methods.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandLombok#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandLombok/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandLombok.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./dist/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "webpack --config webpack.config.js --mode=\"production\"",
		"build-server": "node scripts/build/buildJdtlsExt.js",
		"clean": "node ./node_modules/rimraf/bin.js out/",
		"compile": "tsc -p . && webpack --config webpack.config.js",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"javaExtensions": [
			"./plugins/com.microsoft.java.lombok-0.1.0.jar"
		],
		"languages": [
			{
				"aliases": [
					"Java",
					"java"
				],
				"extensions": [
					".java",
					".jav"
				],
				"id": "java"
			}
		]
	},
	"activationEvents": [
		"onLanguage:java",
		"workspaceContains:pom.xml",
		"workspaceContains:build.gradle"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "^7.1.1",
		"@types/node": "^14.18.21",
		"glob": "^7.1.6",
		"rimraf": "^3.0.1",
		"ts-loader": "^9.3.1"
	},
	"extensionDependencies": [
		"redhat.java"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/icon.png",
	"aiKey": []
}

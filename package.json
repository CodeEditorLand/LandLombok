{
  "name": "vscode-lombok",
  "displayName": "Lombok Annotations Support for VS Code",
  "version": "0.0.1",
  "private": "false",
  "description": "Refactor code with Lombok annotations, or remove Lombok annotations with actual methods.",
  "categories": [
    "Other"
  ],
  "keywords": [
    "lombok",
    "vs-code"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./dist/extension",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=\"production\"",
    "build-server": "node scripts/build/buildJdtlsExt.js",
    "clean": "node ./node_modules/rimraf/bin.js out/",
    "compile": "tsc -p . && webpack --config webpack.config.js",
    "package": "vsce package",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "test": "npm run compile && node ./out/src/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "contributes": {
    "javaExtensions": [
      "./plugins/com.microsoft.java.lombok-0.1.0.jar"
    ],
    "languages": [
      {
        "id": "java",
        "extensions": [
          ".java",
          ".jav"
        ],
        "aliases": [
          "Java",
          "java"
        ]
      }
    ]
  },
  "activationEvents": [
    "onLanguage:java",
    "workspaceContains:pom.xml",
    "workspaceContains:build.gradle"
  ],
  "dependencies": {
    "vscode-extension-telemetry-wrapper": "0.13.3",
    "vscode-languageclient": "9.0.1"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.3",
    "@types/node": "20.8.9",
    "@types/vscode": "1.83.1",
    "@vscode/test-electron": "2.3.6",
    "glob": "10.3.10",
    "mocha": "10.2.0",
    "rimraf": "5.0.5",
    "ts-loader": "9.5.0",
    "tslint": "6.1.3",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "extensionDependencies": [
    "redhat.java"
  ],
  "engines": {
    "vscode": "1.65.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "images/icon.png",
  "aiKey": []
}

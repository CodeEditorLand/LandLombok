{
	"name": "vscode-lombok",
	"displayName": "Lombok Annotations Support for VS Code",
	"version": "0.0.1",
	"private": false,
	"description": "Refactor code with Lombok annotations, or remove Lombok annotations with actual methods.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./dist/extension",
	"scripts": {
		"build": "webpack --config webpack.config.js --mode=\"production\"",
		"build-server": "node scripts/build/buildJdtlsExt.js",
		"clean": "node ./node_modules/rimraf/bin.js out/",
		"compile": "tsc -p . && webpack --config webpack.config.js",
		"package": "vsce package",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "webpack --config webpack.config.js --watch"
	},
	"contributes": {
		"javaExtensions": [
			"./plugins/com.microsoft.java.lombok-0.1.0.jar"
		],
		"languages": [
			{
				"aliases": [
					"Java",
					"java"
				],
				"extensions": [
					".java",
					".jav"
				],
				"id": "java"
			}
		]
	},
	"activationEvents": [
		"onLanguage:java",
		"workspaceContains:pom.xml",
		"workspaceContains:build.gradle"
	],
	"dependencies": {
		"vscode-extension-telemetry-wrapper": "^0.13.2",
		"vscode-languageclient": "7.1.0-next.5"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "^14.18.21",
		"@types/vscode": "^1.65.0",
		"@vscode/test-electron": "^2.3.8",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"rimraf": "^3.0.1",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"typescript-esbuild": "latest",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.10.0"
	},
	"extensionDependencies": [
		"redhat.java"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/icon.png",
	"aiKey": []
}
